<h1>News!!!</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Title</th>
    <th>Content</th>
    <th>User location</th>
    <th>Post location</th>
    <th>Accuracy</th>
    <th>Inaccuracy</th>
    <th>Date</th>
    <th>Time</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @posts.each do |post| %>
  <tr>
    <td><%= link_to  post.name, post  %></td>
    <td><%= post.title %></td>
    <td><%= post.content %></td>
    <td><%= post.user_location %></td>
    <td><%= post.post_location %></td>
    <td><%= post.accuracy %></td>
    <td><%= post.inaccuracy %></td> 
    <td><%= post.date %></td>
    <td><%= post.time %></td>
    <!--<td>%= link_to 'Show', post %></td>
    <td>%= #link_to 'Edit', edit_post_path(post) %></td>-->
    <td><%= link_to 'Delete', post, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Post', new_post_path %>
